# Struct
# --------------------------
# This example shows how to declare a struct and instanciate it.

# Struct declaration
struct Foo
  decl bar: str
end

function main(): none
  # create a new Foo instance and store it into new variable f
  Foo::new => decl f: Foo

  # store "foo" into f.bar
  "foo" => f.bar
  
  # print the value of f.bar
  f.bar puts
end
