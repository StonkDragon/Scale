using "util/array"
using "math"
function createList(len: int): Array
    decl arr:Array len Array:new => arr
    decl i: int 0 => i
    while i len < do
        i len i - arr:set
        i 1 + => i
    done
    arr return
end
function Array:print(): none
    decl i: int 0 => i
    while i self.capacity < do
        i self:get putint
        i 1 + => i
    done
end
function main(): none
