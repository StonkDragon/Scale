union Result
    Ok: any
    Err: any
end

function Result:expected(id: int): none
    assert (id __tag ==) else (
        if id 1 == then
            "Expected Ok, got " self:toString +
        else
            "Expected Err, got " self:toString +
        fi
        AssertError::new throw
    )
end

function Result:toString(): str
    if self.isOk then
        "Ok {" self.Ok:toString + "}" +
    else
        "Err {" self.Err:toString + "}" +
    fi return
end
