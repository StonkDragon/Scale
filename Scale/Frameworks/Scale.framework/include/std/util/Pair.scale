final sealed struct Pair
	decl a: any
	decl b: any

	function init(a: any, b: any): none
		a => self.a
		b => self.b
	end

	function toString(): str
		( ( ("(" a:toString +) (", " b:toString +) +) ")" +) return
	end

	function [](i: int): any
		if i 0 == then
			a return
		elif i 1 == then
			b return
		else
			"Pair index out of bounds: " i + InvalidArgumentException::new throw
		fi
	end
end
